@{
    ViewData["Title"] = "Home Page";
}
@model List<TopicField>

<div class="">
    <h2>Car forum Ukraine</h2>
</div>
<div class="list">
    <p>Topics list</p>
    <ul>
        @foreach (var topic in Model)
        {
            <li><a asp-controller="Topic" asp-action="Page">@topic.QuestionShort</a></li>
        }

    </ul>
    <div>
        <a asp-controller="Topic" asp-action="Add">Add new topic</a>
    </div>
</div>